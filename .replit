modules = ["nodejs-20", "web", "bash"]
[agent]
expertMode = true
integrations = ["github:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["at-spi2-atk", "chromium", "gcc", "glib", "gtk3", "libGL", "libdrm", "libxkbcommon", "mesa", "nginx", "nss", "vulkan-loader", "xorg.libX11", "xorg.libXScrnSaver", "xorg.libXcomposite", "xorg.libXcursor", "xorg.libXdamage", "xorg.libXext", "xorg.libXi", "xorg.libXrandr", "xorg.libXrender", "xorg.libXtst", "xorg.libxcb", "xorg.libxshmfence", "xorg.xorgserver", "xorg.xvfb"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
